        <div class='row'>
            <div class='col-md-2 col-xs-12 hidden-xs-down hidden-md-down ad'>
              <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
              <!-- responsive bar kasi -->
              <ins class="adsbygoogle"
                   style="display:block"
                   data-ad-client="ca-pub-8294995671791919"
                   data-ad-slot="4328078988"
                   data-ad-format="auto"></ins>
              <script>
              (adsbygoogle = window.adsbygoogle || []).push({});
              </script>
            </div>


            <div class='col-md-12 col-lg-8 main'>
                <div class='row'>
                    <div class=' col-xs-12 col-sm-12 col-md-6 col-lg-6'><h1 class='logo'>KRISS </h1></div>
                    <div class='col-xs-12 col-sm-12 col-md-6 col-lg-6'>
                      <div class='col-xs-12 col-sm-12 col-md-6 col-lg-6'>
                         <a href='songlist' class='btn btn-3 btn-3a icon-list-all'>View all</a>
                       </div>
                       <div class='col-xs-12 col-sm-12 col-md-6 col-lg-6'>
                          <a href='upload' class='btn btn-3 btn-3a icon-arrow-up'>Upload &nbsp</a>
                        </div>

                    </div>
                </div>


                <div class='row featured music'>

                    <ul class='collection'>

                        {{#each songs}}
                          <li class='collection-item ' style="background:url({{art}}) center center;background-size:cover;">
                            <div class='heading'>
                              <h2 class="h2">{{name}}</h2>
                              <h3>{{artist}}</h3>
                            </div>
                            <div class='functional'>
                              <a href="{{url}}" download><img id='{{song_id}}' onclick="download(this,'{{url}}')" class='download ' src='img/download.png'></a>
                              <img id='{{song_id}}' onclick="playSound(this,'{{url}}');" class='play' src='img/play.png'  />
                              <img id='{{song_id}}' onclick='flame(this)' ='heat' src='img/heat.png' >
                              <div class='analytics'>
                                <span class='badge badge-pill badge-default download_count'>{{download_count}}</span>'
                                <span class='badge badge-pill badge-default play_count'>{{play_count}}</span>'
                                <span class='badge badge-pill badge-default flame_count'>{{flame_count}}</span>'
                              </div>
                              <div class="hp_slide">
                                  <div class="hp_range"></div>
                              </div>
                            </div>
                          </li>
                          {{/each}}

                    </ul>
                </div>




                <div class='row featured video'>
                    <div class='row video-embed'>
                      <iframe id="ytplayer" type="text/html"
                        src="https://www.youtube.com/embed/{{videos.0.url}}?rel=0&showinfo=0&color=white&iv_load_policy=3"
                        frameborder="0" allowfullscreen class="col-md-12 col-lg1-12 col-sm-12"></iframe>
                    </div>

                </div>

                <div class='row featured events'>
                  <div class='header'>
                    <h1> EVENTS </h1>
                  </div>

                  <div class='event-items'>
                    <ul class='collection'>
                    {{#each events}}
                      <li class='collection-item'>
                        <div class='event-image' style="background:url({{image}}) center center;background-size:cover;">

                        </div>
                        <div class='event-body'>
                          <h5><b>{{name}}</b></h5>
                          <h6>{{venue}}</h6>
                          <h6>{{date}}</h6>
                          <p> {{description}} </p>
                        </div>
                      </li>
                      {{/each}}
                    </ul>
                    </div>
                    <div class='header'>
                      <div class='navbtn'  onclick="loadForm();">
                        <a href='#'>ADD NEW</a>
                      </div>
                      <div class='navbtn'>
                        <a href='#' onclick="listAll()">VIEW ALL</a>
                      </div>
                    </div>
                </div>

                <div class='row featured form-fields'>
                    <div class='col-md-12 col-xs-12'>
                      <form>
                        <div class="form-group">
                          <label for="exampleInputEmail1">Name</label>
                          <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter event name">
                        </div>
                        <div class="form-group">
                          <label for="exampleInputEmail1">Venue</label>
                          <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="e.g Imbizo Club , East London">
                        </div>
                        <div class="form-group row">
                          <label for="example-datetime-local-input" class="col-2 col-form-label">Date and time</label>
                          <div class="col-10">
                            <input class="form-control" type="datetime-local" value="2011-08-19T13:45:00" id="example-datetime-local-input">
                          </div>
                        </div>
                        <div class="form-group">
                          <label for="exampleTextarea">Description</label>
                          <textarea class="form-control" id="exampleTextarea" rows="3"></textarea>
                        </div>
                        <div class="form-group">
                          <label for="exampleInputEmail1">Price</label>
                          <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter ticket fee">
                        </div>
                        <div class="form-group">
                          <label for="exampleInputFile">Image</label>
                          <input type="file" class="form-control-file" id="exampleInputFile" aria-describedby="fileHelp">
                        </div>
                        <div class="form-group">
                          <label for="exampleInputEmail1">Cellphone Number</label>
                          <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Your contact details">
                        </div>
                        <button type="submit" class="btn btn-warning">Submit</button>
                        </form>
                    </div>
                </div>
                <div class='row featured post'></div>


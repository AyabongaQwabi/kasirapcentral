      function switchSide(item){
        console.log($(item))
        if($(item).attr('id') == '0'){
          $(item).parent().addClass('col-md-4')
          $(item).parent().siblings('div').removeClass('col-md-5')
          $(item).parent().removeClass('col-md-7')
          $(item).parent().siblings('div').addClass('col-md-8')
          $(item).html("Make Bigger")
          $(item).attr('id','1')
          $('.video-div').css('height','210px')
          $(".advert iframe").css('width','400x');
        }
        else{
          $(item).parent().addClass('col-md-7')
          $(item).parent().siblings('div').removeClass('col-md-8')
          $(item).parent().removeClass('col-md-4')
          $(item).parent().siblings('div').addClass('col-md-5')
          $(item).html("Make Smaller")
          $(".advert iframe").css('width','468px');
          $(item).attr('id','0')
          $('.video-div').css('height','350px')
        }

        //$(item).toggle();
      }
        function swapContent(item){

          if($(item).children('p').html() == "Olwethu"){
            var url ="url(/img/"+$('.header').html().replace(' ','')+".jpg)"
            $(item).children('p').html($('.header').html())

            $(item).css("backgroundImage",url)
            $('.three').fadeIn(500)
            $('.header').html("Olwethu")
            $('.one').css('display','none');
            $('.two').css('display','none');
            $('.three').css('display','block');
            $('.text').html("Born in Queenstown in the Eastern Cape, Olwethu”s first project in studio once coming to JHB was with Khuli Chana and Wandile known as 1Dile in the music scene.She then worked with Thasman on his album and did t a few racks with him. He also featured Olwethu in a project for Rhythm City.")
          }
          else if($(item).children('p').html() == "Salaphi Sistas"){
            var url ="url(/img/"+$('.header').html().trim().replace(' ','')+".jpg)"
            console.log(url)
            $(item).children('p').html($('.header').html())

            $(item).css("backgroundImage",url)
            $('.two').fadeIn(500)
            $('.one').css('display','none');
            $('.two').css('display','block');
            $('.three').css('display','none');
            $('.header').html("Salaphi Sistas")
            $('.text').html("The Salaphi Sistas are an Afro pop duo comprised   of sisters Enkosi and Tina Salaphi who were born and raised in a small town called Dutywa, Eastern Cape.")
            console.log($(item).children('p').html())
          }
          else{
            var url ="url(/img/"+$('.header').html().replace(' ','')+".jpg)"
            $(item).children('p').html($('.header').html())
            $('.one').fadeIn(500)
            $(item).css("backgroundImage",url)
            $('.one').css('display','block');
            $('.two').css('display','none');
            $('.three').css('display','none');
            $('.header').html("Nomadic Tribe")
            $('.text').html("We are a professional production, composition, sound design and management house. We are passionate about creating unique African sounds and work tirelessly at trying to create contemporary and diverse sounds that reflect the diverse nature of Africa.")
            console.log('Nomadic Tribe')
          }
        }
      </script>
